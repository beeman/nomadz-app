{
  "expo": {
    "name": "nomadz-app",
    "slug": "nomadz-app",
    "owner": "beeman",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "templateexpobasic",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "xyz.nomadz.app",
      "config": {
        "usesNonExemptEncryption": false,
        "googleMapsApiKey": "AIzaSyDMqzecFE82N1YO4a_zQy4I8LWej9dz74Q"
      },
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDMqzecFE82N1YO4a_zQy4I8LWej9dz74Q"
        }
      },
      "edgeToEdgeEnabled": true,
      "package": "xyz.nomadz.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#FFFFFF",
          "dark": {
            "backgroundColor": "#000000",
            "image": "./assets/images/splash-icon.png"
          },
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain"
        }
      ],
      "expo-web-browser",
      "expo-font",
      [
        "expo-maps",
        {
          "requestLocationPermission": true,
          "locationPermission": "Allow $(PRODUCT_NAME) to use your location"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "e436357a-7c39-451b-b65f-30c35942e975"
      },
      "passkeyAssociatedDomain": "https://nomadz.xyz",
      "privyAppId": "cm9qu6suy007fle0lhbc0pst6",
      "privyClientId": "client-WY5iwBYio453ZAPVeLKFNKEMao5ZeJsGNCWwAHjqSayEm",
      "router": {}
    }
  }
}
